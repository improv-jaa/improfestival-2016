{
  "masterPicture": "_assets/img/favicon.png",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#ffffff",
      "margin": "4%",
      "assets": {
        "ios6AndPriorIcons": false,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      },
      "appName": "Tilt 4"
    },
    "desktopBrowser": {},
    "windows": {
      "pictureAspect": "whiteSilhouette",
      "backgroundColor": "#b91d47",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": false,
        "windows10Ie11EdgeTiles": {
          "small": false,
          "medium": true,
          "big": true,
          "rectangle": false
        }
      },
      "appName": "Tilt 4"
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "Tilt 4",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": false
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "silhouette",
      "themeColor": "#5bbad5"
    }
  },
  "settings": {
    "compression": 2,
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": false
  }
}
